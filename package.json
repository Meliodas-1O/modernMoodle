{
  "name": "modernmoodle",
  "version": "1.0.0",
  "description": "modernMoodle",
  "main": "./src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "nodemon ./src/index.ts",
    "dev": "concurrently \"npx tsc -w\" \"nodemon ./src/index.ts\"",
    "migrate": "cp .env ./src/database/ && npx knex migrate:latest --knexfile src/database/knexfile.ts; rm ./src/database/.env",
    "unit-tests": "jest unit",
    "integration-tests": "jest integration",
    "lint": "npx eslint . --max-warnings 1",
    "lint-fix": "npx eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-plugin-typescript": "^0.14.0",
